@model MyProjectTravel.Models.Itinerary

@{
    ViewData["Title"] = "GetItinerarioById";
}

<h1>Itinerario</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Ciudad de Origen
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.origin.city)
        </dd>
        <dt class = "col-sm-2">
            Ciudad de Destino
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.destination.city)
        </dd>
        <dt class = "col-sm-2">
            Trabajador
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.worker.user.userName)
        </dd>
        <dt class = "col-sm-2">
            Bus
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.bus.placa)
        </dd>
        <dt class = "col-sm-2">
            Fecha de Partida
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.startDate)
        </dd>
        <dt class = "col-sm-2">
            Fecha de Llegada
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.arrivalDate)
        </dd>
        <dt class = "col-sm-2">
            Precio
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.price)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Edit", "Edit", new { /* id = Model.PrimaryKey */ }) |
    <a asp-action="Index">Back to List</a>
</div>
